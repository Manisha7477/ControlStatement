package com.kodnest.Practice;
import java.util.Scanner;
public class Application13 {
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		System.out.println("Mani! please enter the number");
		int a = s.nextInt();
		if(a%4==0 && a%3==0)
		{
			System.out.println("Number is muiltiple of 4 & divisible by 3");
		}
		else
		{
			System.out.println("Number is not multiple of 4 & also not divisible by 3");
		}
	}

}
